<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="450.0" prefWidth="550.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.CompanyViewController">
    <top>
        <BorderPane>
            <right>
                <Button mnemonicParsing="false" text="Return" BorderPane.alignment="CENTER" />
            </right>
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
            <BorderPane.margin>
                <Insets right="10.0" />
            </BorderPane.margin>
         <left>
            <Button mnemonicParsing="false" text="Button" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets bottom="10.0" left="10.0" top="10.0" />
               </BorderPane.margin>
            </Button>
         </left>
         <center>
            <HBox alignment="CENTER" BorderPane.alignment="CENTER">
               <children>
                  <Button alignment="CENTER" disable="true" mnemonicParsing="false" text="Edit" />
               </children>
               <padding>
                  <Insets bottom="10.0" top="10.0" />
               </padding>
            </HBox>
         </center>
        </BorderPane>
    </top>

    <center>

    </center>
    <left>
        <TableView prefHeight="405.0" prefWidth="350.0" BorderPane.alignment="CENTER">
            <columns>
                <TableColumn prefWidth="199.0" text="Employees" />
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
    </left>
   <right>
      <VBox prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </right>

</BorderPane>
